[".emacs.d"]
    type = "git-repo"
    url = "https://github.com/syl20bnr/spacemacs"
[".config/tmux/plugins/tpm"]
    type = "archive"
    url = "https://codeload.github.com/tmux-plugins/tpm/zip/refs/heads/master"
    exact = true
    stripComponents = 1
{{ if eq .chezmoi.os "darwin" }}["Library/Fonts"]{{ else }}[".fonts"]{{ end }}
    type = "archive"
    url = "https://github.com/ryanoasis/nerd-fonts/releases/latest/download/Meslo.zip"
{{ if eq .chezmoi.os "darwin" }}["Library/Fonts/mesloLGS_NF_regular.ttf"]{{ else }}[".fonts/mesloLGS_NF_regular.ttf"]{{ end }}
    type = "file"
    url = "https://github.com/IlanCosman/tide/blob/assets/fonts/mesloLGS_NF_regular.ttf?raw=true"
{{ if eq .chezmoi.os "darwin" }}["Library/Fonts/mesloLGS_NF_bold.ttf"]{{ else }}[".fonts/mesloLGS_NF_bold.ttf"]{{ end }}
    type = "file"
    url = "https://github.com/IlanCosman/tide/blob/assets/fonts/mesloLGS_NF_bold.ttf?raw=true"
{{ if eq .chezmoi.os "darwin" }}["Library/Fonts/mesloLGS_NF_italic.ttf"]{{ else }}[".fonts/mesloLGS_NF_italic.ttf"]{{ end }}
    type = "file"
    url = "https://github.com/IlanCosman/tide/blob/assets/fonts/mesloLGS_NF_italic.ttf?raw=true"
{{ if eq .chezmoi.os "darwin" }}["Library/Fonts/mesloLGS_NF_bold_italic.ttf"]{{ else }}[".fonts/mesloLGS_NF_bold_italic.ttf"]{{ end }}
    type = "file"
    url = "https://github.com/IlanCosman/tide/blob/assets/fonts/mesloLGS_NF_bold_italic.ttf?raw=true"
{{ if ne .chezmoi.os "windows" }}
[".oh-my-zsh"]
    type = "archive"
    url = "https://github.com/ohmyzsh/ohmyzsh/archive/master.tar.gz"
    stripComponents = 1
[".oh-my-zsh/custom/plugins/zsh-syntax-highlighting"]
    type = "archive"
    url = "https://github.com/zsh-users/zsh-syntax-highlighting/archive/master.tar.gz"
    stripComponents = 1
[".oh-my-zsh/custom/themes/powerlevel10k"]
    type = "archive"
    url = "https://github.com/romkatv/powerlevel10k/archive/v1.19.0.tar.gz"
    stripComponents = 1
[".config/fish"]
    type = "archive"
    url = "https://github.com/ilancosman/tide/archive/master.tar.gz"
    stripComponents = 1
    include = ["*/completions/**", "*/conf.d/**", "*/functions/**"]
[".config/fish/themes/Catppuccin Mocha.theme"]
    type = "file"
    url = "https://raw.githubusercontent.com/catppuccin/fish/main/themes/Catppuccin%20Mocha.theme"
[".oh-my-bash"]
    type = "archive"
    url = "https://github.com/ohmybash/oh-my-bash/archive/master.zip"
    stripComponents = 1
{{ end }}
